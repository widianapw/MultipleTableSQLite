        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".features.student_crud.show_student_list.StudentListActivity" android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".features.student_crud.update_student_info.StudentUpdateDialogFragment" />
        <activity android:name=".features.student_crud.create_student.StudentCreateDialogFragment" />
        <activity android:name=".features.prodi_crud.update_prodi_info.ProdiUpdateDialogFragment" />
        <activity android:name=".features.prodi_crud.show_prodi_list.ProdiListActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".features.prodi_crud.create_prodi.ProdiCreateDialogFragment">

        </activity>

    </application>

</manifest>